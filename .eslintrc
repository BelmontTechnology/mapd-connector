{
    "extends": "airbnb",
    "ecmaFeatures": {
        "classes": true,
        "defaultParams": true,
        "modules": true
    },

    "env": {
        "browser": true,
        "es6": true
    },

    "rules": {
        "no-undef":[0],
        "no-unused-vars": [1],
        "no-console": [0],
        "no-param-reassign": [0],
        "no-use-before-define": [0]
    }
}
